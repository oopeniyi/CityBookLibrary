@model Entities.Models.ApplicationUser

@{
    ViewData["Title"] = "Register";
}

<h4>User Registration</h4>


<form method="post" asp-controller="Home" asp-action="Register">
    <div class="form-group col-sm-6">
        <label for="Name">First Name</label>
        <input type="text" class="form-control" id="FirstName" asp-for="FirstName" placeholder="Enter first name">
    </div>   
    <div class="form-group col-sm-6">
        <label for="EmailId">Email Id</label>
        <input type="email" class="form-control" id="EmailId" asp-for="Email" placeholder="Enter email id">
    </div>   
    <div class="form-group col-sm-6">
        <label for="Password">Password</label>
        <input type="password" class="form-control" id="Password" asp-for="Password" placeholder="Enter password">
    </div>
    <div class="form-group col-sm-6">
        <a asp-action="Index" class="btn btn-danger">Cancel</a>
        <button type="submit" class="btn btn-primary">Register</button>
    </div>
</form> 